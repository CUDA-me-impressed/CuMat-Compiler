func int[*] head(int[*] list) {
    [x:_] = list
    return x
}

func int[*] tail (int[*] list) {
    [_:xs] = list
    return xs
}

func int fold1((int,int) -> int folder, int state, int[1] foldable) {
    return if foldable == [] then
                state
           else
                fold1 (folder, folder(state,head(foldable)), tail(foldable))
}